{
  "manifest_version": 3,
  "name": "Türkçe Karakter Kopyalayıcı",
  "version": "1.2.1",
  "description": "Türkçe karakterleri hızlıca panoya kopyalamak için bir araç.",
"icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
},
